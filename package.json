{
  "name": "stars-vacation-management-v2",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/startup-log.cjs && next dev -p 3000",
    "dev:ready": "concurrently -k -r \"npm run dev\" \"npm run wait:health\"",
    "wait:health": "node scripts/wait-on-http.mjs",
    "kill:3000": "./scripts/kill-3000.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "setup:production": "node setup-production-env.js",
    "deploy:firebase": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "env:check": "node scripts/env-check.js",
    "dev:env-check": "node scripts/print-env.cjs",
    "test:firestore:admin": "node test-firestore-integration.cjs",
    "dev:e2e": "cross-env E2E_USE_FAKE=1 next dev -p 3000",
    "test": "vitest run",
    "test:e2e": "cross-env E2E_USE_FAKE=1 PLAYWRIGHT_BASE_URL=http://localhost:3000 playwright test",
    "seed:admins": "tsx scripts/seed-admins.ts",
    "preview:build": "npm run dev:env-check && npm run build",
    "knip": "knip",
    "tsprune": "ts-prune -p tsconfig.json",
    "depcheck": "depcheck --skip-missing=true",
    "cleanup:report": "npm run knip && npm run tsprune && npm run depcheck",
    "build:check": "npm run lint:fix && next build",
    "translate-missing": "tsx src/i18n/translate-missing.ts"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.19",
    "dotenv": "^17.1.0",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "googleapis": "^150.0.1",
    "next": "^15.5.0",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.5",
    "nodemailer": "^6.10.1",
    "playwright": "^1.55.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@mswjs/interceptors": "^0.39.6",
    "@playwright/test": "^1.55.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.32",
    "eslint-plugin-unused-imports": "^4.2.0",
    "firebase-tools": "^14.11.2",
    "knip": "^5.63.0",
    "msw": "^2.11.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
