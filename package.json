{
  "name": "stars-vacation-management",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:ready": "concurrently -k -r \"npm run dev\" \"npm run wait:health\"",
    "wait:health": "node scripts/wait-on-http.mjs",
    "kill:3000": "./scripts/kill-3000.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "setup:production": "node setup-production-env.js",
    "deploy:firebase": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "knip": "knip",
    "tsprune": "ts-prune -p tsconfig.json",
    "depcheck": "depcheck --skip-missing=true",
    "cleanup:report": "npm run knip && npm run tsprune && npm run depcheck",
    "build:check": "npm run lint:fix && next build"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.17",
    "dotenv": "^17.1.0",
    "firebase": "^11.10.0",
    "googleapis": "^150.0.1",
    "next": "^15.5.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.32",
    "eslint-plugin-unused-imports": "^4.2.0",
    "firebase-tools": "^14.11.2",
    "knip": "^5.63.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-prune": "^0.10.3",
    "typescript": "^5.8.3"
  }
}
