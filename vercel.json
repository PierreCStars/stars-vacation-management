{
  "framework": "nextjs",
  "buildCommand": "rm -rf .next node_modules/.cache && npm ci && npm run build",
  "installCommand": "npm ci",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "stars-vacation-management.vercel.app" }],
      "destination": "https://starsvacationmanagementv2.vercel.app/$1",
      "permanent": true
    }
  ]
}
